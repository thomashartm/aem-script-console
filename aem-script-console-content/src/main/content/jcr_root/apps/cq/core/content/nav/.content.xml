                    jcr:mixinTypes="[rep:AccessControllable]"
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Asset Reports"
                    href="/libs/dam/gui/content/reports/reportspage.html"
                    id="cq-assets-assetreports">
                <rep:policy/>
            </assetreports>
            <videoreports
                    jcr:description="View Video Reports"
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Video Reports"
                    href="/libs/dam/gui/content/s7dam/videoreports/videodetailreport.html"
                    id="cq-assets-videoreports"/>
