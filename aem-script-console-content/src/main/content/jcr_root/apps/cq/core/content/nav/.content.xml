                        jcr:primaryType="nt:unstructured"
                        jcr:title="Packages"
                        href="/crx/packmgr"
                        id="granite-packages"
                        target="_blank"/>
                <packageshare
                        jcr:description="Download applications from Adobe and the community"
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Package Share"
                        href="/crx/packageshare"
                        id="granite-packageshare"
                        target="_blank"/>
            </packaging>
            <deployment
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Deployment"
                    id="granite-deployment">
                <clustering
                        jcr:description="Manage clustered deployments"
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Clustering"
                        href="/libs/granite/cluster/content/admin.html"
                        id="granite-clustering"
                        target="_blank"/>
                <topology
                        jcr:description="View the instances topology"
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Topology"
                        href="/libs/granite/topology/content/view.html"
                        id="granite-topology"/>
                <offloading
                        jcr:description="Manage instances offloading"
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Offloading"
                        href="/libs/granite/offloading/content/view.html"
                        id="granite-offloading"/>
                <ocs
                        jcr:description="Cloud-based infrastructure deployment"
                        jcr:primaryType="nt:unstructured"
                        jcr:title="One-Click Setup"
                        href="/ocs.html"
                        id="granite-ocs"/>
