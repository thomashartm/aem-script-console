                    jcr:description="Maintain and configure your system"
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Configuration"
                    href="/miscadmin"
                    id="cq-tools-configuration"
                    target="_blank"/>
            <backup
                    jcr:description="Carry out backup tasks"
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Backup"
                    href="/libs/granite/backup/content/admin.html"
                    id="granite-backup"
                    target="_blank"/>
            <rep:policy/>
