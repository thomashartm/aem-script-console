            <viewerpresets
                    jcr:description="Manage your viewer rendition presets"
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Viewer Presets"
                    href="/libs/dam/gui/content/s7dam/viewerpresets/viewerpresets.html"
                    id="cq-assets-viewerpresets"/>
