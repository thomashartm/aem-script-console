<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:rep="internal"
          jcr:primaryType="nt:unstructured"
          id="root">
    <projects
            jcr:primaryType="nt:unstructured"
            jcr:title="Projects"
            desktopOnly="/welcome"
            desktopOnlyTitle="Classic UI"
            href="/projects.html"
            id="cq-projects"/>
    <sites
            jcr:mixinTypes="[rep:AccessControllable]"
            jcr:primaryType="nt:unstructured"
            jcr:title="Sites"
            desktopOnly="/siteadmin#"
            desktopOnlyTitle="Classic UI"
            href="/sites.html"
            id="cq-sites">
        <rep:policy/>
    </sites>
    <apps
            jcr:mixinTypes="[rep:AccessControllable]"
            jcr:primaryType="nt:unstructured"
            jcr:title="Apps"
            href="/aem/apps.html"
            id="cq-mobile">
        <rep:policy/>
    </apps>
    <publications
            jcr:mixinTypes="[rep:AccessControllable]"
            jcr:primaryType="nt:unstructured"
            jcr:title="Publications"
            desktopOnly="/publishingadmin#"
            desktopOnlyTitle="Classic UI"
            href="/aem/publications.html"
            id="cq-publications">
        <rep:policy/>
    </publications>
    <forms
            jcr:primaryType="nt:unstructured"
            jcr:title="Forms"
            href="/aem/forms.html"
            id="cq-forms"/>
    <assets
            jcr:primaryType="nt:unstructured"
            jcr:title="Assets"
            desktopOnly="/damadmin#"
            desktopOnlyTitle="Classic UI"
            href="/assets.html"
            id="cq-assets"/>
    <communities
            jcr:mixinTypes="[rep:AccessControllable]"
            jcr:primaryType="nt:unstructured"
            jcr:title="Communities"
            href="/communities.html"
            id="cq-communities">
        <rep:policy/>
    </communities>
    <commerce
            jcr:mixinTypes="[rep:AccessControllable]"
            jcr:primaryType="nt:unstructured"
            jcr:title="Commerce"
            id="cq-commerce">
        <catalogs
                jcr:primaryType="nt:unstructured"
                jcr:title="Catalogs"
                desktopOnly="/miscadmin#/etc/commerce"
                desktopOnlyTitle="Classic UI"
                href="/aem/catalogs.html"
                id="cq-commerce-catalogs"/>
        <products
                jcr:description="Manage your product marketing data"
                jcr:primaryType="nt:unstructured"
                jcr:title="Products"
                desktopOnly="/miscadmin#/etc/commerce/products"
                desktopOnlyTitle="Classic UI"
                href="/aem/products.html"
                id="cq-commerce-products"/>
        <orders
                jcr:description="Manage your orders"
                jcr:primaryType="nt:unstructured"
                jcr:title="Orders"
                desktopOnly="/miscadmin#/etc/commerce/orders"
                desktopOnlyTitle="Classic UI"
                href="/aem/orders.html"
                id="cq-commerce-orders"/>
        <rep:policy/>
    </commerce>
