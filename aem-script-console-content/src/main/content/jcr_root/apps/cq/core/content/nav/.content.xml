                    jcr:primaryType="nt:unstructured"
                    jcr:title="Processing Profiles"
                    id="cq-assets-processingprofiles">
                <overview
                        jcr:description="Manage your processing profiles"
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Overview"
                        href="/libs/dam/gui/content/processingprofilepage.html"
                        id="cq-assets-processingprofiles-overview"/>
                <metadata
                        jcr:description="Manage your metadata profiles"
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Metadata Profiles"
                        href="/libs/dam/gui/content/processingprofilepage/metadataprofiles.html"
                        id="cq-assets-processingprofiles-metadata"/>
                <image
                        jcr:description="Manage your image profiles"
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Image Profiles"
                        href="/libs/dam/gui/content/imageprocessingprofiles.html"
                        id="cq-assets-processingprofiles-image"/>
