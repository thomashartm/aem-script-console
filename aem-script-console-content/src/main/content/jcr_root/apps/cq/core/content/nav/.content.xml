                    jcr:description="Manage metadata schema forms"
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Metadata Schemas"
                    href="/libs/dam/gui/content/metadataschemaeditor/schemalist.html"
                    id="cq-assets-metadataschemas"/>
