        <resources
                jcr:primaryType="nt:unstructured"
                jcr:title="Resources"
                id="cq-tools-resources">
            <documentation
                    jcr:description="View the Web Experience Management documentation"
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Documentation"
                    href_i18n="https://www.adobe.com/go/aem-docs"
                    id="cq-documentation"
                    target="_blank"/>
            <dev
                    jcr:description="Developer resources and downloads"
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Developer Resources"
                    href_i18n="https://www.adobe.com/go/aem-dev"
                    id="cq-dev"
                    target="_blank"/>
        </resources>
        <crxdelite
                jcr:description="Develop applications with a web-based IDE"
                jcr:primaryType="nt:unstructured"
                jcr:title="CRXDE Lite"
                href="/crx/de"
                id="granite-crxdelite"
                target="_blank"/>
        <webconsole
                jcr:description="Configure and manage the application platform"
                jcr:primaryType="nt:unstructured"
                jcr:title="Web Console"
                href="/system/console/configMgr"
                id="granite-webconsole"
                target="_blank"/>
