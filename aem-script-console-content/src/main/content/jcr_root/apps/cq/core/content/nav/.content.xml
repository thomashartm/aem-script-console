                    jcr:description="Manage your marketing launches"
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Launches"
                    href="/libs/launches/content/admin.html"
                    id="cq-tools-launches"
                    target="_blank"/>
            <replication
                    jcr:description="Create and manage multiple websites"
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Replication"
                    href="/etc/replication.html"
                    id="cq-tools-replication"
                    target="_blank"/>
