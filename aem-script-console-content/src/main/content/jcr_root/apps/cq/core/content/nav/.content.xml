            jcr:primaryType="nt:unstructured"
            jcr:title="Tools"
            id="cq-tools">
        <operations
                jcr:mixinTypes="[rep:AccessControllable]"
                jcr:primaryType="nt:unstructured"
                jcr:title="Operations"
                id="cq-tools-operations">
            <dashboard
                    jcr:description="Health reports, diagnosis tools and automated maintenance tasks"
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Dashboard"
                    href="/system/operations.html"
                    id="granite-monitoring">
                <healthreports
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Health Reports"
                        href="/libs/granite/operations/content/healthreports.html"
                        id="healthreports"/>
                <diagnosis
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Diagnosis"
                        href="/libs/granite/operations/content/diagnosis.html"
                        id="diagnosis"/>
                <maintenance
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Maintenance"
                        href="/libs/granite/operations/content/maintenance.html"
                        id="maintenance"/>
            </dashboard>
            <security
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Security"
                    id="granite-security">
                <users
                        jcr:description="Manage your users"
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Users"
                        desktopOnly="/useradmin"
                        desktopOnlyTitle="Classic UI"
                        href="/libs/granite/security/content/useradmin.html"
                        id="granite-users"/>
                <groups
                        jcr:description="Manage your groups"
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Groups"
                        desktopOnly="/useradmin"
                        desktopOnlyTitle="Classic UI"
                        href="/libs/granite/security/content/groupadmin.html"
                        id="granite-groups"/>
