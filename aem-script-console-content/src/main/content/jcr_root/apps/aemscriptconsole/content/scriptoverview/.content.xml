<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0"
          xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="cq:Page">
    <jcr:content
            jcr:mixinTypes="[sling:VanityPath]"
            jcr:primaryType="nt:unstructured"
            jcr:title="Yet another AEM script console"
            sling:resourceType="granite/ui/components/shell/collectionpage"
            sling:vanityOrder="{Long}100"
            sling:vanityPath="/aem/scriptoverview"
            consoleId="asfc-script-overview"
            modeGroup="asfc-script-overview-admin-childpages"
            pageURITemplate="/aem/scriptoverview.html{+id}"
            targetCollection=".asfc-script-overview-admin-childpages">
        <redirector
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/admin/contentchecker"
                allowed="^(/etc/aemscriptconsole(.*))$"
                path="${requestPathInfo.suffix}"
                redirectWhenEmpty="/aem/scriptoverview.html/etc/aemscriptconsole"/>
        <head jcr:primaryType="nt:unstructured">
            <clientlibs
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/includeclientlibs"
                    categories="[coralui3,granite.ui.coral.foundation]"/>
        </head>
        <views jcr:primaryType="nt:unstructured">
            <card
                    granite:rel="asfc-script-overview-childpages"
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Card View"
                    sling:resourceType="granite/ui/components/coral/foundation/masonry"
                    icon="viewCard"
                    limit="{Long}20"
                    selectionCount="single"
                    selectionMode="{Boolean}false"
                    metaResourceType="cq/gui/components/coral/admin/page/meta"
                    modeGroup="asfc-script-overview-admin-childpages"
                    path="${requestPathInfo.suffix}"
                    size="${empty requestPathInfo.selectors[1] ? &quot;20&quot; : requestPathInfo.selectors[1]}"
                    src="/mnt/overlay/wcm/core/content/sites/jcr:content/views/card{.offset,limit}.html{+id}">
                <datasource
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="cq/gui/components/common/wcm/datasources/childpages"
                        applyWeighting="{Boolean}true"
                        itemResourceType="aemscriptconsole/components/scriptpage"
                        limit="${empty requestPathInfo.selectors[1] ? &quot;21&quot; : requestPathInfo.selectors[1] + 1}"
                        offset="${requestPathInfo.selectors[0]}"
                        path="${requestPathInfo.suffix}"/>
            </card>
        </views>
        <actions jcr:primaryType="nt:unstructured">
            <primary jcr:primaryType="nt:unstructured"/>
            <secondary jcr:primaryType="nt:unstructured"/>
            <selection jcr:primaryType="nt:unstructured">
                <disable
                        granite:class="foundation-toggleable-control"
                        granite:id="agent-disable-activator"
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/anchorbutton"
                        icon="minusCircle"
                        text="Disable"
                        title="Disable"
                        variant="actionBar">
                    <granite:data
                            jcr:primaryType="nt:unstructured"
                            foundation-toggleable-control-action="show"
                            foundation-toggleable-control-target="#agentDisableModal"/>
                </disable>
                <enable
                        granite:class="foundation-toggleable-control"
                        granite:hidden="{Boolean}true"
                        granite:id="agent-enable-activator"
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/anchorbutton"
                        icon="checkCircle"
                        text="Enable"
                        title="Enable"
                        variant="actionBar">
                    <granite:data
                            jcr:primaryType="nt:unstructured"
                            foundation-toggleable-control-action="show"
                            foundation-toggleable-control-target="#agentEnableModal"/>
                </enable>
                <pause
                        granite:class="foundation-toggleable-control"
                        granite:hidden="{Boolean}true"
                        granite:id="agent-pause-activator"
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/anchorbutton"
                        href="#agentPauseModal"
                        icon="pauseCircle"
                        text="Pause"
                        title="Pause"
                        toggle="modal"
                        variant="actionBar">
                    <granite:data
                            jcr:primaryType="nt:unstructured"
                            foundation-toggleable-control-action="show"
                            foundation-toggleable-control-target="#agentPauseModal"/>
                </pause>
                <resume
                        granite:class="foundation-toggleable-control"
                        granite:hidden="{Boolean}true"
                        granite:id="agent-resume-activator"
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/anchorbutton"
                        href="#agentResumeModal"
                        icon="pausePlayCircle"
                        text="Resume"
                        title="Resume"
                        toggle="modal"
                        variant="actionBar">
                    <granite:data
                            jcr:primaryType="nt:unstructured"
                            foundation-toggleable-control-action="show"
                            foundation-toggleable-control-target="#agentResumeModal"/>
                </resume>
            </selection>
        </actions>
        <breadcrumbs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wcm/commons/ui/shell/datasources/breadcrumbs"
                path="${requestPathInfo.suffix}"
                rootTitle="Script Pages"/>
    </jcr:content>
    <previewcolumn
            jcr:primaryType="nt:unstructured"
            sling:resourceType="aemscriptconsole/components/scriptpagepreview"
            path="${requestPathInfo.suffix}"/>
</jcr:root>